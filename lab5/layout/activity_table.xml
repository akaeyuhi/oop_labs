<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".TableActivity">

    <Button
            android:text="@string/return_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/to_editor_button"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.95" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.976"/>
    <Button
            android:text="@string/save_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/save"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.223" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.976"/>
    <com.example.lab3oop.ShapeTable
            android:layout_width="411dp"
            android:layout_height="656dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.356" app:layout_constraintBottom_toTopOf="@+id/to_editor_button"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/shapeTable"/>
</androidx.constraintlayout.widget.ConstraintLayout>