            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.976"/>
    <com.example.lab3oop.ShapeTable
            android:layout_width="383dp"
            android:layout_height="655dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.356" app:layout_constraintBottom_toTopOf="@+id/save"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/shapeTable" android:layout_marginStart="10dp" android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp" android:layout_marginBottom="10dp"/>
